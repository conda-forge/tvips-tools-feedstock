   OPTIONAL)
 install(
   FILES
+    "${PROJECT_BINARY_DIR}/ht2wavelength.1"
     "${PROJECT_BINARY_DIR}/tiff2smv.1"
     "${PROJECT_BINARY_DIR}/tvips2smv.1"

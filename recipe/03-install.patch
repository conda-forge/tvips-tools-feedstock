 add_executable(img2img
   "compat.c"
   "frame.c"
@@ -345,6 +352,7 @@ add_executable(mike_test
   "util.c")
 add_dependencies(mike_test _update_version_h)
 target_link_libraries(mike_test m)
+endif()
 
 add_executable(mrc2smv
   "compat.c"
@@ -356,6 +364,7 @@ add_executable(mrc2smv

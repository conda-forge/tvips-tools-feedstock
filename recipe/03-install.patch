+set(BUILD_JIFFIES OFF CACHE BOOL
+  "Build MicroED developer jiffies, not for distribution")
+if(BUILD_JIFFIES)
 add_executable(cphdr
   "compat.c"
   "cphdr.c"
@@ -252,6 +255,7 @@ add_executable(dan_test
   "util.c")
